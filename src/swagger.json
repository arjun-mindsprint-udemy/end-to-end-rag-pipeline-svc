{
    "swagger": "2.0",
    "info": {
        "title": "Sample Node.js API",
        "version": "1.0.0"
    },
    "paths": {
        "/data-prep": {
            "get": {
                "summary": "Data Preparation API",
                "responses": {
                    "200": {
                        "description": "Returns chunks of text."
                    }
                }
            }
        },
        "/embedChunks": {
            "get": {
                "summary": "Chunk Embedding API",
                "responses": {
                    "200": {
                        "description": "Returns embeddings of chunks of text."
                    }
                }
            }
        },
        "/embedQuery": {
            "get": {
                "summary": "Query Embedding API",
                "responses": {
                    "200": {
                        "description": "Returns embeddings of query."
                    }
                }
            }
        },
        "/upsert": {
            "get": {
                "summary": "Upsert API",
                "responses": {
                    "200": {
                        "description": "Returns success after upserting text."
                    }
                }
            }
        },
                "/query": {
            "get": {
                "summary": "Query API",
                "responses": {
                    "200": {
                        "description": "Returns similar chunks of text based on a query."
                    }
                }
            }
        },
                        "/llm": {
            "get": {
                "summary": "LLM API",
                "responses": {
                    "200": {
                        "description": "Returns response from LLM based on query and context."
                    }
                }
            }
        },
        "/health": {
            "get": {
                "summary": "Health Check"
            }
        }
    }
}